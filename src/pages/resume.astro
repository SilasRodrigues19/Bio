---
import Layout from "../layouts/Layout.astro";

import pdf from '/resume.pdf';

---
<Layout
  title='Bio - Silas Rodrigues | Resume.'
  description='Resume page'
>
  <section class="bg-gray-100 text-gray-800 dark:bg-gray-800 dark:text-gray-100 py-16 sm:py-20">
    <div class="mx-auto max-w-2xl px-4 text-left sm:px-6 lg:max-w-7xl lg:px-8 mt-12">
      <embed src={pdf} type="application/pdf" width="100%" height="600px" />
    </div>
  </section>
</Layout>

